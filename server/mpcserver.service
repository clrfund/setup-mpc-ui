[Service]
ExecStart=/usr/bin/node /root/setup-mpc-ui/server/index.js
WorkingDirectory=/root/setup-mpc-ui/server
Restart=always
StandardOutput=syslog
SyslogIdentifier=mpcserver
User=root
Group=root
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production

[Install]
WantedBy=multi-user.target
